<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>

<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
	<head>
		<title>符号页面</title>
		<style type="text/css">
			.edit{width: 1000px; height: 500px;border: 1px solid #000; margin-left: auto; margin-right: auto;  margin-top: 100px;
				/* Rotate div 
				transform:rotate(30deg);
				-ms-transform:rotate(30deg); /* IE 9  
				-moz-transform:rotate(30deg); /* Firefox  
				-webkit-transform:rotate(30deg); /* Safari and Chrome  
				-o-transform:rotate(30deg); /* Opera */
							
			}
			.edit .hat{width: 10px; height: 5px; background-color: #0080FF; position: relative; top: 30px; left: 50px;}
			.edit .body{width: 10px; height: 50px; background-color: #000; position: relative; top: 32px; left: 50px;}
			.edit .tip{width:0; height:0; border-left:5px solid #FFF; border-right:5px solid #FFF; border-top:15px solid #0080ff; border-bottom: 0px solid #0080ff; position: relative; top: 35px; left: 50px;}
			
			.edit .delete{width:3px; height:1; border-left:15px solid #000; border-right:15px solid #000; border-top:15px solid #0080ff; border-bottom: 15px solid #0080ff; background-color: #0080ff; position: relative; top: 50px; left: 50px;}
		</style>
	</head>

	<body>
		<div class="edit">
			<div class="hat"></div>
			<div class="body"></div>
			<div class="tip"></div>
			
			
			<div class="delete"></div>
		</div>
	</body>
</html>
