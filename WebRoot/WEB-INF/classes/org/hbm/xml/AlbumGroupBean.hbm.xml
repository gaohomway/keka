<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.keekan.bean">
	<class name="AlbumGroupBean" table="album_group">
		<id	name="albumGroupId" column="album_group_id">
			<generator class="identity"/>
		</id>
		<property name="keeno" column="keeno" type="integer" length="50" not-null="false" unique="false" update="true" insert="true"/>
		<property name="albumGroupName" column="album_group_name" type="string" length="100" not-null="false" unique="false" update="true" insert="true"/>
	</class>	
</hibernate-mapping>